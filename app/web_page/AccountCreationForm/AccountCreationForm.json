    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.account_info ){",
        "\treturn {",
        "\t\t//map account_info variable to expected task contract input",
        "\t\taccount_infoInput: {",
        "\t\t\taccount_username: $data.account_info.account_username !== undefined ? $data.account_info.account_username : null,",
        "\t\t\taccount_password: $data.account_info.account_password !== undefined ? $data.account_info.account_password : null,",
        "\t\t\taccount_creationDate: $data.account_info.account_creationDate !== undefined ? $data.account_info.account_creationDate : null,",
        "\t\t\taccount_clientID: $data.account_info.account_clientID !== undefined ? $data.account_info.account_clientID : null,",
        "\t\t\taccount_clientname: $data.account_info.account_clientname !== undefined ? $data.account_info.account_clientname : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
