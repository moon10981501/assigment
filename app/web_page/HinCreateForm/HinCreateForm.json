    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.slipstream && $data.slipstream_account_information ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.slipstream.account_information = $data.slipstream_account_information;",
        "\treturn {",
        "\t\t//map slipstream variable to expected task contract input",
        "\t\tslipstreamInput: {",
        "\t\t\tclient_name: $data.slipstream.client_name !== undefined ? $data.slipstream.client_name : null,",
        "\t\t\tclient_ID: $data.slipstream.client_ID !== undefined ? $data.slipstream.client_ID : null,",
        "\t\t\thin_exist: $data.slipstream.hin_exist !== undefined ? $data.slipstream.hin_exist : null,",
        "\t\t\thin_number: $data.slipstream.hin_number !== undefined ? $data.slipstream.hin_number : null,",
        "\t\t\tdaily_transaction_limit: $data.slipstream.daily_transaction_limit !== undefined ? $data.slipstream.daily_transaction_limit : null,",
        "\t\t\tasset_transfer_requested: $data.slipstream.asset_transfer_requested !== undefined ? $data.slipstream.asset_transfer_requested : null,",
        "\t\t\tclient_existing_broker: $data.slipstream.client_existing_broker !== undefined ? $data.slipstream.client_existing_broker : null,",
