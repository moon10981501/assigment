    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.mega_sales_smasher_robot ){",
        "\treturn {",
        "\t\t//map mega_sales_smasher_robot variable to expected task contract input",
        "\t\tmega_sales_smasher_robotInput: {",
        "\t\t\tdate_signing: $data.mega_sales_smasher_robot.date_signing !== undefined ? $data.mega_sales_smasher_robot.date_signing : null,",
        "\t\t\tbroker_name: $data.mega_sales_smasher_robot.broker_name !== undefined ? $data.mega_sales_smasher_robot.broker_name : null,",
        "\t\t\tbroker_ID: $data.mega_sales_smasher_robot.broker_ID !== undefined ? $data.mega_sales_smasher_robot.broker_ID : null,",
        "\t\t\tclient_ID: $data.mega_sales_smasher_robot.client_ID !== undefined ? $data.mega_sales_smasher_robot.client_ID : null,",
        "\t\t\tclient_name: $data.mega_sales_smasher_robot.client_name !== undefined ? $data.mega_sales_smasher_robot.client_name : null,",
        "\t\t\tbonus_amount: $data.mega_sales_smasher_robot.bonus_amount !== undefined ? $data.mega_sales_smasher_robot.bonus_amount : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "context" : {
      "type" : "url",
      "value" : [
        "../API/bpm/userTask/{{taskId}}/context"
      ],
      "exposed" : false
    },
